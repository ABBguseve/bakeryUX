GAAA,CAAA,aAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,QAAA;;AACA,WAAA,IAAA,CAAA,IAAA,SAAA,EAAA;AACA,YAAA,KAAA,IAAA,IAAA,SAAA,CAAA,CAAA,CAAA,CAAA,KAAA,EAAA;AACA,cAAA,KAAA,QAAA,IAAA,SAAA,CAAA,CAAA,CAAA,CAAA,QAAA,EAAA;AACA,iBAAA,WAAA,GAAA,SAAA,CAAA,CAAA,CAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,SAAA,CAAA,CAAA,CAAA,CAAA,IAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,WAAA;AACA,WAJA,MAIA;AACA,YAAA,OAAA,CAAA,KAAA,CAAA,sBAAA;AACA;AACA;AACA;;AACA,MAAA,YAAA,CAAA,OAAA,CACA,kBADA,EAEA,IAAA,CAAA,SAAA,CAAA,KAAA,WAAA,CAFA;AAIA,WAAA,YAAA,GAAA,KAAA;AACA,KAxBA;AAyBA,IAAA,MAzBA,oBAyBA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,MAAA,YAAA,CAAA,KAAA;AACA;AA/BA;AAjBA,CAAA","sourcesContent":["<template>\r\n  <v-app>\r\n    <v-toolbar class=\"px-15 fixed-bar\" flat>\r\n      <v-toolbar-title style=\"font-family: 'Lobster', cursive\">\r\n        <h2>Bakeriet</h2>\r\n      </v-toolbar-title>\r\n      <v-spacer></v-spacer>\r\n      <v-btn class=\"navBtn text-capitalize\" plain to=\"/\" color=\"black\"\r\n        >Start</v-btn\r\n      >\r\n      <v-btn class=\"navBtn text-capitalize\" plain to=\"/varor\" color=\"black\"\r\n        >Varor</v-btn\r\n      >\r\n      <v-btn class=\"navBtn text-capitalize\" plain to=\"/best\" color=\"black\"\r\n        >Beställningar</v-btn\r\n      >\r\n      <v-btn class=\"navBtn text-capitalize\" plain to=\"/omoss\" color=\"black\"\r\n        >Om oss</v-btn\r\n      >\r\n      <v-btn\r\n        class=\"navBtn ml-2\"\r\n        rounded\r\n        depressed\r\n        small\r\n        color=\"primary\"\r\n        dark\r\n        @click=\"login_dialog = !login_dialog\"\r\n        v-if=\"!currentUser\"\r\n      >\r\n        Logga in\r\n      </v-btn>\r\n      <v-avatar\r\n        class=\"mx-2\"\r\n        v-else\r\n        clickable\r\n        @click=\"konto_dialog = !konto_dialog\"\r\n      >\r\n        <img :src=\"currentUser.avatar\" />\r\n      </v-avatar>\r\n    </v-toolbar>\r\n    <v-main>\r\n      <router-view />\r\n    </v-main>\r\n    <div v-if=\"!currentUser\" class=\"text-center\">\r\n      <v-dialog v-model=\"login_dialog\" width=\"400\">\r\n        <v-card>\r\n          <v-card-title class=\"navBtn headline grey lighten-2\">\r\n            Inloggning\r\n          </v-card-title>\r\n          <v-text-field\r\n            class=\"px-12 fontStyle_1\"\r\n            label=\"E-post\"\r\n            hide-details=\"auto\"\r\n            v-model=\"mail\"\r\n          ></v-text-field>\r\n          <v-spacer></v-spacer>\r\n          <v-text-field\r\n            type=\"password\"\r\n            class=\"px-12 fontStyle_1\"\r\n            label=\"Lösenord\"\r\n            hide-details=\"auto\"\r\n            v-model=\"password\"\r\n          ></v-text-field>\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn\r\n              class=\"navBtn my-3 mr-8\"\r\n              rounded\r\n              depressed\r\n              small\r\n              color=\"primary\"\r\n              dark\r\n              @click=\"logIn\"\r\n            >\r\n              Logga in\r\n            </v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-dialog>\r\n    </div>\r\n    <div v-else class=\"text-center\">\r\n      <v-dialog v-model=\"konto_dialog\" width=\"400\">\r\n        <v-card>\r\n          <v-card-title class=\"navBtn headline grey lighten-2\">\r\n            Konto\r\n          </v-card-title>\r\n          <h4 class=\"px-12 navBtn\">Namn</h4>\r\n          <v-spacer></v-spacer>\r\n          <h4 class=\"px-12 fontStyle_1\">{{ currentUser.name }}</h4>\r\n          <v-spacer></v-spacer>\r\n          <v-divider></v-divider>\r\n          <h4 class=\"px-12 navBtn\">E-post</h4>\r\n          <v-spacer></v-spacer>\r\n          <h4 class=\"px-12 fontStyle_1\">{{ currentUser.email }}</h4>\r\n          <v-spacer></v-spacer>\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn\r\n              class=\"navBtn my-3 mr-8\"\r\n              rounded\r\n              depressed\r\n              small\r\n              color=\"primary\"\r\n              dark\r\n              @click=\"logOut\"\r\n            >\r\n              Logga ut\r\n            </v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-dialog>\r\n    </div>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"App\",\r\n\r\n  data: () => ({\r\n    login_dialog: false,\r\n    konto_dialog: false,\r\n    customers: {},\r\n    currentUser: null,\r\n    mail: \"\",\r\n    password: \"\"\r\n  }),\r\n  mounted() {\r\n    axios\r\n      .get(\"http://localhost:3000/customers\")\r\n      .then(response => (this.customers = response.data));\r\n    this.currentUser = JSON.parse(localStorage.getItem(\"Storage_customer\"));\r\n  },\r\n  methods: {\r\n    logIn() {\r\n      let customers = this.customers;\r\n      console.log(\"customers\");\r\n      console.log(this.customers);\r\n      console.log(\"currentuser\");\r\n      console.log(this.mail);\r\n      console.log(this.password);\r\n      for (let c in customers) {\r\n        if (this.mail == customers[c].email) {\r\n          if (this.password == customers[c].password) {\r\n            this.currentUser = customers[c];\r\n            console.log(customers[c].name);\r\n            console.log(\"logged in\");\r\n          } else {\r\n            console.error(\"no matching password\");\r\n          }\r\n        }\r\n      }\r\n      localStorage.setItem(\r\n        \"Storage_customer\",\r\n        JSON.stringify(this.currentUser)\r\n      );\r\n      this.login_dialog = false;\r\n    },\r\n    logOut() {\r\n      this.mail = null;\r\n      this.password = null;\r\n      this.currentUser = null;\r\n      this.konto_dialog = false;\r\n      localStorage.clear();\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.navBtn {\r\n  font-family: \"Lobster\", cursive;\r\n}\r\n.fontStyle_1 {\r\n  font-family: \"Poppins\", sans-serif;\r\n}\r\n.fixed-bar {\r\n  position: sticky;\r\n  position: -webkit-sticky; /* for Safari */\r\n  top: 6em;\r\n  z-index: 2;\r\n}\r\n</style>\r\n"],"sourceRoot":"src"}]}